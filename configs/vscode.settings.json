{
  "[jsonc]": {
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.suggest.insertMode": "replace",
    "gitlens.codeLens.scopes": [
      "document"
    ]
  },
  "[python]": {
    "editor.formatOnType": true
  },
  "codeium.enableConfig": {
    "*": true,
    "markdown": true
  },
  "dev.containers.executeInWSL": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.useInlineViewWhenSpaceIsLimited": false,
  "diffEditor.wordWrap": "on",
  "editor.acceptSuggestionOnEnter": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.fontFamily": "MesloLGS Nerd Font, MesloLGS NF, Menlo",
  "editor.fontLigatures": true,
  "editor.fontSize": 12,
  "editor.guides.bracketPairs": true,
  "editor.guides.bracketPairsHorizontal": true,
  "editor.guides.highlightActiveBracketPair": true,
  "editor.hover.delay": 1000,
  "editor.minimap.enabled": false,
  "editor.rulers": [100],
  "editor.stickyScroll.enabled": false,
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "explorer.autoReveal": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.trimTrailingWhitespace": true,
  "git.allowForcePush": true,
  "git.autofetch": true,
  "git.confirmForcePush": false,
  "git.confirmSync": false,
  "git.openRepositoryInParentFolders": "always",
  "git.terminalAuthentication": false,
  "gitlens.advanced.messages": {
    "suppressGitMissingWarning": true,
    "suppressLineUncommittedWarning": true
  },
  "go.formatTool": "goimports",
  "go.toolsManagement.autoUpdate": true,
  "go.useLanguageServer": true,
  "redhat.telemetry.enabled": false,
  "remote.SSH.configFile": "~/.ssh/config",
  "remote.SSH.remotePlatform": {
    "mothership": "linux",
    "stargate": "linux",
    "watchtower": "linux"
  },
  "remote.SSH.suppressWindowsSshWarning": true,
  "remote.SSH.useExecServer": true,
  "remote.SSH.useLocalServer": false,
  "scm.showOutgoingChanges": "never",
  "security.workspace.trust.enabled": false,
  "security.workspace.trust.untrustedFiles": "open",
  "settingsSync.ignoredSettings": [],
  "sync.autoDownload": true,
  "sync.autoUpload": true,
  "sync.quietSync": true,
  "terminal.external.osxExec": "iTerm.app",
  "terminal.external.windowsExec": "C:\\WINDOWS\\System32\\wsl.exe",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.defaultProfile.windows": "Debian (WSL)",
  "terminal.integrated.fontFamily": "MesloLGS Nerd Font, MesloLGS NF, Menlo",
  "terminal.integrated.fontSize": 12,
  "terminal.integrated.profiles.linux": {
    "bash": {
      "icon": "terminal-bash",
      "path": "bash"
    },
    "fish": {
      "icon": "terminal-bash",
      "path": "fish"
    },
    "pwsh": {
      "icon": "terminal-powershell",
      "path": "pwsh"
    },
    "tmux": {
      "icon": "terminal-tmux",
      "path": "tmux"
    },
    "zsh": {
      "icon": "terminal-bash",
      "path": "zsh"
    }
  },
  "terminal.integrated.profiles.windows": {
    "Command Prompt": {
      "args": [],
      "icon": "terminal-cmd",
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ]
    },
    "Git Bash": {
      "source": "Git Bash"
    },
    "PowerShell": {
      "icon": "terminal-powershell",
      "source": "PowerShell"
    }
  },
  "vim.useSystemClipboard": true,
  "vs-kubernetes": {
    "vs-kubernetes.crd-code-completion": "disabled",
    "vs-kubernetes.draft-path.mac": "/Users/jay/.vs-kubernetes/tools/draft/darwin-amd64/draft",
    "vs-kubernetes.helm-path.mac": "/opt/homebrew/bin/helm",
    "vs-kubernetes.kubectl-path.mac": "/opt/homebrew/bin/kubectl",
    "vs-kubernetes.minikube-path.mac": "/opt/homebrew/bin/minikube",
    "vscode-kubernetes.helm-path.linux": "/home/jay/.vs-kubernetes/tools/helm/linux-amd64/helm",
    "vscode-kubernetes.helm-path.mac": "/opt/homebrew/bin/helm",
    "vscode-kubernetes.kubectl-path.linux": "/home/jay/.vs-kubernetes/tools/kubectl/kubectl",
    "vscode-kubernetes.kubectl-path.mac": "/opt/homebrew/bin/kubectl",
    "vscode-kubernetes.minikube-path.linux": "/home/jay/.vs-kubernetes/tools/minikube/linux-amd64/minikube",
    "vscode-kubernetes.minikube-path.mac": "/opt/homebrew/bin/minikube",
    "vscode-kubernetes.minikube-path-mac": "/Users/jay/.vs-kubernetes/tools/minikube/darwin-arm64/minikube"
  },
  "workbench.editorAssociations": {
    "*.plist": "default"
  },
  "workbench.iconTheme": "material-icon-theme",
  "workbench.startupEditor": "none"
}