# Ignore all non-dotfiles (files/dirs not starting with .)

^[^.].*

# Ignore markdown files (including dotfiles with .md anywhere)

\.md$

# Ignore specific dotfiles that should stay in .jsh

^\.bin
^\.envrc
^\.fzf
^\.git
^\.gitmodules
^\.stow-local-ignore
^\.stignore
^\.stfolder
^\.DS_Store
^\.taskfiles

# Ignore sync conflict files

\.sync-conflict-.*

# Ignore temporary and backup files

~$
\.bak$
\.swp$
\.tmp$
